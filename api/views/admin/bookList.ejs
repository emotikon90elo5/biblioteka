<!doctype html>
<html lang="pl">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Rent Book - Panel Zarządania - Lista Książek</title>
  <link rel="shortcut icon" type="image/png" href="/imgs/logo.png" />
  <link rel="stylesheet" href="/assets/css/styles.min.css" />
  <script src="/assets/js/Books.js" defer></script>
  <% if (typeof id !='undefined' ) { %>
    <script>
      const id = <%=id%>
    </script>
    <% } else{%>
    <script>
      const id = -1;
    </script>
    <%}%> <% if (typeof data.title !='undefined' ) { %>
    <script>
      const title = "<%=data.title%>";
    </script>
    <% } else{%>
    <script>
      const title = "";
    </script>
    <%}%> <% if (typeof data.author !='undefined' ) { %>
    <script>
      const author = "<%=data.author%>";
    </script>
    <% } else{%>
    <script>
      const author = "";
    </script>
    <%}%> <% if (typeof data.publishinghouse !='undefined' ) { %>
    <script>
      const publishinghouse = "<%=data.publishinghouse%>";
    </script>
    <% } else{%>
    <script>
      const publishinghouse = "";
    </script>
    <%}%> <% if (typeof data.name !='undefined' ) { %>
    <script>
      const name = "<%=data.name%>";
    </script>
    <% } else{%>
    <script>
      const name = "";
    </script>
    <%}%>
</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->
      <div>
        <div class="brand-logo d-flex align-items-center justify-content-between">
          <a href="/" class="text-nowrap logo-img">
            <img src="/imgs/logoB.png" width="100%" alt="" />
          </a>
          <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
            <i class="ti ti-x fs-8"></i>
          </div>
        </div>
        <!-- Sidebar navigation-->
        <%- include("sideBarNav") -%>
        <!-- End Sidebar navigation -->
      </div>
      <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
      <!--  Header Start -->
      <header class="app-header">
        <nav class="navbar navbar-expand-lg navbar-light">
          <ul class="navbar-nav">
            <li class="nav-item d-block d-xl-none">
              <a class="nav-link sidebartoggler nav-icon-hover" id="headerCollapse" href="javascript:void(0)">
                <i class="ti ti-menu-2"></i>
              </a>
            </li>
          </ul>
          <div class="navbar-collapse justify-content-end px-0" id="navbarNav">
            <ul class="navbar-nav flex-row ms-auto align-items-center justify-content-end">
              <a href="/api/auth/logout" target="_blank" class="btn btn-primary">Wyloguj się!</a>
            </ul>
          </div>
        </nav>
      </header>
      <!--  Header End -->
      <div class="container-fluid">
        <div class="container-fluid">
          <div class="card">
            <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Czy chcesz usunąć</h5>
                      <a aria-hidden="true" id="closePopup">&times;</a><!-- add css to it -->
                  </div>
                  <div class="modal-body" id="exampleModalLongTitleContent">
                    ...
                  </div>
                  <div class="modal-footer">
                    
                    <button type="button" class="btn btn-danger" id="Cancel" >Anuluj</button>
                    <button type="button" class="btn btn-success" id="Commit" data-dismiss="modal">Usuń</button>
                  </div>
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="card">
                <div class="card-body">
              <form >
                <div class="row mb-2">
                  <div class="col">
                    <input type="text"name="title" id="title" class="form-control" placeholder="Tytuł">
                  </div>
                  <div class="col">
                    <input type="text" name="author" id="author" class="form-control" placeholder="Autor">
                  </div>
                </div>
                <div class="row mb-2">
                  <div class="col">
                    <input type="text" name="publishinghouse" id="publishinghouse" class="form-control" placeholder="Wydawnictwo">
                  </div>
                  <div class="col">
                    <input type="text" name="name" id="type" class="form-control" placeholder="Gatunek">
                  </div>
                </div>
                <button type="submit" class="btn btn-primary">Szukaj!</button>
              </form></div></div>
              <h5 class="card-title fw-semibold mb-4">wybierz książkę</h5>
              <div class="card">

                <div class="card-body">
                  
                    <table   class="table table-bordered table-hover"> 
                      <thead>
                        <tr> 
                          <th>
                          Tytuł
                          </th>
                          <th>
                            Autor
                          </th>
                          <th>
                            Kod Ksiązki
                          </th>
                          <th>
                            Akcja 
                          </th>
                        </tr>
                      </thead>
                      <tbody id="table">

                      </tbody>
                        
                    </table>
  
                  
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  
  <script src="/assets/libs/jquery/dist/jquery.min.js"></script>
  <script src="/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/assets/js/sidebarmenu.js"></script>
  <script src="/assets/js/app.min.js"></script>
  <script src="/assets/libs/simplebar/dist/simplebar.js"></script>
  <!-- <script src="/assets/js/dashboard.js"></script> -->
</body>

</html>
